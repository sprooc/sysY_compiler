decl @getint(): i32
decl @getch(): i32
decl @getarray(*i32): i32
decl @putint(i32)
decl @putch(i32)
decl @putarray(i32, *i32)
decl @starttime()
decl @stoptime()
fun @main(): i32 {
%entry:
  @x_0 = alloc i32
  store 10, @x_0
  %0 = load @x_0
  %1 = add %0, 1
  store %1, @x_0
  %2 = load @x_0
  ret %2
}
  .text
  .global main
main:
  sub   sp, sp, 16
  li    t0, 10
  sw    t0, 0(sp)
  sw    t0, sp, 0
  lw    t0, 0(sp)
  lw    t0, sp, 0
  sw    t0, 4(sp)
  sw    t0, sp, 4
  lw    t0, 4(sp)
  lw    t0, sp, 4
  li    t1, 1
  add   t0, t0, t1
  sw    t0, 8(sp)
  sw    t0, sp, 8
  lw    t0, 8(sp)
  lw    t0, sp, 8
  sw    t0, 0(sp)
  sw    t0, sp, 0
  lw    t0, 0(sp)
  lw    t0, sp, 0
  sw    t0, 12(sp)
  sw    t0, sp, 12
  mv    a0, x0
  ret
