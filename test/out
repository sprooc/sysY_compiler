decl @getint(): i32
decl @getch(): i32
decl @getarray(*i32): i32
decl @putint(i32)
decl @putch(i32)
decl @putarray(i32, *i32)
decl @starttime()
decl @stoptime()
fun @main(): i32 {
%entry:
  @x_0 = alloc i32
  store 1, @x_0
  %0 = load @x_0
  ret %0
}
  .text
  .global main
main:
  addi  sp, sp, -16
  li    t0, 1
  sw    t0, 0(sp)
  lw    t0, 0(sp)
  sw    t0, 4(sp)
  lw    a0, 4(sp)
  addi  sp, sp, 16
  ret
