decl @getint(): i32
decl @getch(): i32
decl @getarray(*i32): i32
decl @putint(i32)
decl @putch(i32)
decl @putarray(i32, *i32)
decl @starttime()
decl @stoptime()
fun @half(@x: i32): i32 {
%L0:
  @x_0 = alloc i32
  store @x, @x_0
  %0 = load @x_0
  ret %0
}
fun @main(): i32 {
%entry:
  @a_1 = alloc i32
  %2 = load @a_1
  %1 = call @half(%2)
  ret %1
}
  .text
  .global half
half:
  addi  sp, sp, -16
L0:
  sw    a1, 0(sp)
  lw    t0, 0(sp)
  sw    t0, 4(sp)
  lw    a0, 4(sp)
  addi  sp, sp, 16
  ret

  .text
  .global main
main:
  addi  sp, sp, -16
  sw    ra, 12(sp)
entry:
  lw    t0, 0(sp)
  sw    t0, 4(sp)
  lw    a0, 4(sp)
  call  half
  sw    a0, 8(sp)
  lw    a0, 8(sp)
  lw    ra, 12(sp)
  addi  sp, sp, 16
  ret

