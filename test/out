  .text
  .global half
half:
  addi  sp, sp, -16
L0:
  sw    a1, 0(sp)
  lw    t0, 0(sp)
  sw    t0, 4(sp)
  lw    a0, 4(sp)
  addi  sp, sp, 16
  ret

  .text
  .global main
main:
  addi  sp, sp, -16
  sw    ra, 12(sp)
entry:
  lw    t0, 0(sp)
  sw    t0, 4(sp)
  call  half
  sw    a0, 8(sp)
  lw    a0, 8(sp)
  lw    ra, 12(sp)
  addi  sp, sp, 16
  ret

